{
  "name": "typeson-registry",
  "version": "1.0.0-alpha.27",
  "description": "The type registry for typeson",
  "author": "dfahlander",
  "contributors": [
    "Brett Zamir"
  ],
  "main": "./dist/all.js",
  "module": "./index.js",
  "scripts": {
    "prepublishOnly": "yarn",
    "eslint": "eslint .",
    "start": "static -p 8085",
    "test": "npm run rollup && npm run eslint && mocha test/test-node",
    "browser-test": "npm run rollup && npm run eslint && opn http://localhost:8085/browser-test/ && npm start",
    "rollup": "node ./build.js",
    "windows": "node windows-devinstall"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfahlander/typeson-registry.git"
  },
  "browserslist": [
    "cover 100%"
  ],
  "keywords": [
    "typeson",
    "JSON",
    "remoting",
    "universal"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dfahlander/typeson-registry/issues"
  },
  "homepage": "https://github.com/dfahlander/typeson-registry#readme",
  "engines": {
    "node": "x"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "canvas": "2.4.1",
    "chai": "4.2.0",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-compat": "3.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "jsdom": "14.0.0",
    "mocha": "6.0.2",
    "node-static": "0.7.11",
    "opn-cli": "^4.0.0",
    "rollup": "1.7.4",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-resolve": "4.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-uglify": "6.0.2",
    "uglify-es": "3.3.9"
  },
  "dependencies": {
    "base64-arraybuffer-es6": "0.4.2",
    "typeson": "5.12.0",
    "uuid": "3.3.2",
    "whatwg-url": "7.0.0"
  },
  "tonicExample": "var Typeson = require('typeson');\nvar TSON = new Typeson().register(require('typeson-registry/dist/presets/builtin'));\n\nTSON.stringify({foo: new Date()}, null, 2);"
}
